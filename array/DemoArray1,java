class DemoArray1
{
	public static void main(String args[])
	{
		int arr[]={22,33,44,55,66,77,88}
		int len=arr.length;
		System.out.println("Length is :" +len);
		for(int i=0; i<len; i++);
		{
			System.out.println("arr ["+i+"] is:" +arr[i]);
		}
		System.out.println("End of programe:");
	}
	
}